cars
-
make VARCHAR PK
model VARCHAR PK
year INT PK
mpg INT
fuel VARCHAR
size_class VARCHAR
cylinders INT
displacement_liters DECIMAL
drive VARCHAR
transmisson VARCHAR
super_charger BOOLEAN
turbo_charger BOOLEAN


craigs_list
-
make VARCHAR FK - cars.make
model VARCHAR FK - cars.model
year INT FK - cars.year
craigslist_price INT
image_url VARCHAR
description VARCHAR
paint_color VARCHAR
state VARCHAR FK >- gas_cost.state
city VARCHAR	
lat FLOAT
lng FLOAT


auction_list
-
make VARCHAR FK - cars.make
model VARCHAR FK - cars.model
year INT FK - cars.year
color VARCHAR
auction_price INT
state VARCHAR FK >- gas_cost.state
lat FLOAT
lng FLOAT

gas_cost
-
state VARCHAR
regular FLOAT
mid-grade FLOAT
premium FLOAT
diesel FLOAT
